class Solution {
    public long removeZeros(long n) {
         long res =0;
         long place =1;
         while(n>0){
            int digits = n % 10;
            if(digits != 0){
                res = res + digit * place; //reversing it kinda
                place *= 10; 
            }
            n?=10;
         }
         return res;
    }
}

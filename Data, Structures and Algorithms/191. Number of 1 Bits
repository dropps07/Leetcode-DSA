class Solution {
    public int hammingWeight(int n) {
        String res = "";
        while(n!=1){
            if(n%2!=1){
                res += "0";
            }else {
                res+= "1";
            }
            n/=2;
        }
        int count=0;
        for(int i=0;i<res.length();i++){
            char bit = res.charAt(i);
            if(bit == '1'){
                count++;
            }
        }
        return count+1;
    }
}

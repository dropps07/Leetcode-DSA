class Solution {
    public int singleNumber(int[] nums) {
        //brute force 
        int n = nums.length;
        Map<Integer, Integer> maps = new HashMap<>();
        for(int x: nums){
            //counting freq
            maps.put(x, maps.getOrDefault(x, 0)+1); //maps.getOrDefault(x, 0)
        // → Look up key x in the map. → If it exists, return its current count.
        // → If it doesn’t exist, return 0 instead of null. +1 Increment that count by 1
        }
        for(Map.Entry<Integer, Integer> entry : maps.entrySet()){
            if(entry.getValue() == 1){
                return entry.getKey();
            }
        }
        return -1;
    }
}

class Solution {
    public List<Integer> findMissingElements(int[] nums) {
        Set<Integer> seen = new HashSet<>();
        List<Integer> res = new ArrayList<>();
        if(nums == null || nums.length ==0) return res;
        Arrays.sort(nums);
        int n = nums.length;
        for(int x: nums){
            seen.add(x);
        }
        int start =nums[0];
        int end = nums[nums.length-1];
        for(int i=start;i<=end;i++){
            if(!seen.contains(i)){
                res.add(i);
            }
        }
        return res;
    }
}

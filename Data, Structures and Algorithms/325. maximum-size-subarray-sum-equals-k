class solution {
public int maximunSizeSubArray(int nums[], int k){
int sum=0;
Map<Integer, Integer> map = new HashMap<>();
map.put(0,-1);
int max =0;
for(int i =0;i<nums.length;i++){
sum+=nums[i];
}
int x = sum - k;
if(map.containsKey(x)){
max = Math.max(i-map.get(i), max);
}
if(!map.containsKey(sum)){
map.put(sum, i);
}
return max;
}
}

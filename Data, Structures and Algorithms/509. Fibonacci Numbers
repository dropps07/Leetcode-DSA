class Solution {
    public int fib(int n) {
        //edge cases
        if(n<=1) return n;
        //arr for memoization
        int arr[] = new int[n+1];
        //base cases
        arr[0] = 0;
        arr[1] = 1;
        //iterating through rest of the values
        for(int i=2;i<=n;i++){
            arr[i]= arr[i-1] + arr[i-2];
        }
        return arr[n];
    }
}

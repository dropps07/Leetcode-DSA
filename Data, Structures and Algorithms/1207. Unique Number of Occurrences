class Solution {
    public boolean uniqueOccurrences(int[] arr) {
        Set<Integer> set= new HashSet<>();
        Map<Integer, Integer> map = new HashMap<>();
        for(int i: arr){
            map.put(i, map.getOrDefault(i,0)+1);
        }
        for(int value: map.values()){
            if(set.contains(value)){
                return false;
            }
            set.add(value);
        }
        return true;
    }
}

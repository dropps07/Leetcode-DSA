class Solution {
    public int singleNumber(int[] nums) {
        int MAX_BIT = 32;
        int singleNo=0;
        int[] count = new int[MAX_BIT];
        for(int i: nums ){
            for(int j=0;j<MAX_BIT;j++)
                if((i & (1<<j)) != 0 ){
                    count[j]++;
                }
            }
        for(int i=0;i<32;i++){
            if(count[i] % 3 != 0)
                singleNo |= (1<<i);
        }
        return singleNo;
    }
}

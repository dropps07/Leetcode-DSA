class Solution{
public int findNumbers(int nums[]){
int c=0;
for(int i=0;i<nums.length;i++){
int digits = String.valueOf(nums[i]).length();       //ive another solution for this for(int i: nums){ while(i!=0){i/=10; c++;}if(c % 2==0)}{digits++;}{return digits;}
if(digits % 2 ==0){
c++:
}
}
return c;
}
}


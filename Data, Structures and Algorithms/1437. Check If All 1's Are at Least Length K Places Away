class solution{
   public bollean kLengthApart(int [] nums, int k){
      int l = nums.length;
      int prev = -1;
      for(int i=0;i<l;i++){
          if(nums[i] == 1){
              if(prev != -1 && i - prev - 1 < k){
                  return false;
              }
            }
          prev=i;
      }
      return true;
   }
}
